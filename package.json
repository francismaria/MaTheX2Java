{
  "name": "mathex2java-translator",
  "version": "1.0.0",
  "description": "Transpiler to convert amsmath LaTeX to Java code.",
  "main": "mathex2java.js",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage",
    "build": "webpack"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 90,
        "lines": 90
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francismaria/MaTheX2Java.git"
  },
  "keywords": [
    "latex",
    "mathex2java",
    "java",
    "transpiler",
    "compiler",
    "antlr4",
    "languages",
    "cfg"
  ],
  "author": "Francisco Maria",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/francismaria/MaTheX2Java/issues"
  },
  "homepage": "https://github.com/francismaria/MaTheX2Java#readme",
  "devDependencies": {
    "jest": "^26.6.3",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "antlr4": "^4.7.2"
  }
}
